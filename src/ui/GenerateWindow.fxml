<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="261.0" prefWidth="454.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TextField fx:id="recordsTF" layoutX="117.0" layoutY="93.0" prefHeight="25.0" prefWidth="221.0" promptText="records to generate" />
      <Text layoutX="82.0" layoutY="161.0" strokeType="OUTSIDE" strokeWidth="0.0" text="If left on blank, the default is set on one million">
         <font>
            <Font size="14.0" />
         </font>
      </Text>
      <Text layoutX="152.0" layoutY="59.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Generate Records">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Text>
      <Button fx:id="generateBTN" layoutX="195.0" layoutY="191.0" mnemonicParsing="false" onAction="#generateRecords" style="-fx-background-color: #3d9fff;" text="Generate" textFill="WHITE" />
      <Button fx:id="backBTN" layoutX="21.0" layoutY="27.0" mnemonicParsing="false" onAction="#goBack" style="-fx-background-color: #e04343;" text="Back" textFill="WHITE" />
   </children>
</AnchorPane>
