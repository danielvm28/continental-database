<AnchorPane prefHeight="207.0" prefWidth="478.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TextField fx:id="recordsTF" layoutX="21.0" layoutY="79.0" prefHeight="25.0" prefWidth="159.0" promptText="Records to generate" />
      <Text layoutX="167.0" layoutY="33.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Generate Records">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Text>
      <Button fx:id="generateBTN" layoutX="201.0" layoutY="79.0" mnemonicParsing="false" onAction="#generateRecords" style="-fx-background-color: #3d9fff;" text="Generate" textFill="WHITE" />
      <Button fx:id="backBTN" layoutX="21.0" layoutY="14.0" mnemonicParsing="false" onAction="#goBack" style="-fx-background-color: #e04343;" text="Back" textFill="WHITE" />
      <ProgressBar fx:id="progressBar" layoutX="21.0" layoutY="150.0" prefHeight="25.0" prefWidth="338.0" progress="0.0" />
      <Button fx:id="generateDefBTN" layoutX="301.0" layoutY="79.0" mnemonicParsing="false" onAction="#generateDefault" style="-fx-background-color: #3d9fff;" textFill="WHITE" />
      <Separator layoutX="21.0" layoutY="121.0" prefHeight="11.0" prefWidth="442.0" />
      <Separator layoutX="281.0" layoutY="70.0" orientation="VERTICAL" prefHeight="43.0" prefWidth="0.0" />
      <ImageView fx:id="checkIcon" fitHeight="49.0" fitWidth="33.0" layoutX="431.0" layoutY="146.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@check_icon.png" />
         </image>
      </ImageView>
      <Text fx:id="timeText" layoutX="21.0" layoutY="192.0" strokeType="OUTSIDE" strokeWidth="0.0">
         <font>
            <Font size="13.0" />
         </font>
      </Text>
      <Label fx:id="progressPercentageText" layoutX="370.0" layoutY="154.0" prefHeight="17.0" prefWidth="33.0" />
   </children>
</AnchorPane>
